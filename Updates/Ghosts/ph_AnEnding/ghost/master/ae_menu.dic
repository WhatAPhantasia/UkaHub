	"\![*]\q[What you're doing,OnSnoopingOoh]\n" 	//eventual snoop function hehehehehe
	
	"\n\f[align,right]\![*]\q[Back to Main Menu,OnMenuRun]  \![*]\q[Cancel,CANCEL]\e"
}

