//function for the USER playing a card in eights
OnEightsPlay : all
{
	//reference0: played rank (can also be "draw", etc)
	//reference1: played suit
	
	_playcard = "%(reference0),%(reference1)"
	_displaycard == ""
	_i == 0
	
	case _playcard[0] {
		when "A" {_displaycard = "Ace"}
		when "Q" {_displaycard = "Queen"}
		when "K" {_displaycard = "King"}
		when "J" {_displaycard = "Jack"}
		others {_displaycard = "%(_playcard[0])"}}
	if _displaycard != "JOKER"; _displaycard += " of %(_displaycard[1])"
	//else; _displaycard = "JOKER"
	
	"\0\t\*\s[1]\i[86]\i[35]"
	
	//DEBUG
	"debug: %(_playcard)\n%(deck[(ARRAYSIZE(deck)-1)])\n%(discard[(ARRAYSIZE(discard)-1)])\n\n"
	
	//ehhhhhh edge cases to deal with are if a joker is at the top of the discard pile......
	//thats actually easy to deal with because the discard suit will be JOKER rather than what you'd set the suit to when a joker is pulled, soooo yay!
	if reference0 == "draw"
	{
		discard ,= deck[(ARRAYSIZE(deck)-1)]
		deck[(ARRAYSIZE(deck)-1)] == IARRAY
		_displaycard == discard[(ARRAYSIZE(discard)-1)]
		
		if ARRAYSIZE(deck) < 1
		{
			"\_q(You take a %(_displaycard) from the deck, \w2which just so happens to be the last card!)\w8\n\n"
			if uhand >= 40; "\s[3]\i[86]\i[26]\_q%(vera)\_q ...%(pername), what are you doing..?? \w8um,"
			else; "\s[2]\i[86]\_q%(vera)\_q oh, dang!"
			" \w6\s[14]give me a second to shuffle the discard pile again...\x\![raise,OnEights,redodeck]"
		}
		else
		{"\_q(You take a %(_displaycard) from the deck.)\x\![raise,OnEights]"}
	}
	else
	{
		if _playcard != discard[(ARRAYSIZE(discard)-1)] && (reference0 != "JOKER" || reference0 != "8" || reference0 != "2" || reference0 != "Q")
		{"\_q(You can't play your %(_displaycard) because the suit doesn't match the discard!)\x\![raise,OnEights]"}
		else 
		{
			_i = ASEARCH(_playcard,uhand)
			discard ,= uhand[_i]
			uhand[_i] = IARRAY
			//-------
			if ARRAYSIZE(uhand) < 1
			{"\_q(You play your last card- %(_displaycard)! \w8YOU WIN!!)\x\![raise,Oneightsinit,userwin]"}
			elseif reference0 == "JOKER" //wild +2
			{"\_q(You play your JOKER!! \w8What suit would you like to change to?)\x\![raise,OnEightsSuit,user,joker]"}
			elseif reference0 == "8" //wild
			{"\_q(You play your %(TOUPPER(_displaycard))! \w8What suit would you like to change to?)\x\![raise,OnEightsSuit,user]"}
			elseif reference0 == "2" //+2
			{"\_q(You play your %(TOUPPER(_displaycard))! \w8Vera is forced to draw two cards!)\x\![raise,OnEightsVera,two]"}
			elseif reference0 == "Q" //skip
			{"\_q(You play your %(TOUPPER(_displaycard))! \w8Vera's turn is skipped!)\x\![raise,OnEights,skip]"}
			else
			{"\_q(You place your %(_displaycard) on the discard pile and end your turn.)\x\![raise,OnEightsVera]"}
		}
	}
}
