OnAiTalk
{
	if CHAIN.IDName == ""
	{
		if reference0 == "talk"; LastTalk = RandomTalk
		elseif reference0 == "text"; LastTalk = PhoneTalk
		else; LastTalk = ChooseTalk
	}
	else
	{
		LastTalk = ChainTalk
	}
	LastTalk
}

ChooseTalk
{
	// i dont like this system but it works for now... sigh
	if RAND(4) == 0; PhoneTalk
	else; RandomTalk
}

